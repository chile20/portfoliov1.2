    "gatsby-plugin-typography": "^2.5.0",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.2",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-responsive-iframe": "^2.4.2",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.0",
    "jshint": "^2.11.0",
    "prismjs": "^1.20.0",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^2.0.8",
    "react-helmet": "6.0.0",
    "react-helmet-async": "^1.0.5",
    "react-is": "^16.8.6",
    "react-toggle": "^4.1.1",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.0",
    "tailwindcss": "^1.3.5",
    "typeface-poppins": "0.0.72",
    "typewriter-effect": "^2.13.0",
    "typography": "^0.16.19",
    "use-dark-mode": "^2.3.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "^2.8.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.19.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "tailwind.macro": "^0.5.10",
    "typescript": "^3.8.3"
